void DC_PlayString(const char* str, unsigned char* segShow)
{
	const int seg_size = 80;

	if (strlen(str) >= 128)
	{
		U8 part[128];
		U8* p = strchr(str, '|');
		U8* p_next;
		U8* p_start = str;
		int brk = 0;

		for (; brk!= 1;)
		{
			p_next = strchr(p + 1, '|');
			if (!p_next)
			{
				brk = 1;
			}
			if (brk != 1)
			{
				if (p_next >= p_start + seg_size)
				{
					if (str == p_start)
					{
						strncpy(part, p_start, p - p_start);
						part[p - p_start] = '\0';
					}
					else
					{
						part[0] = '&';
						strncpy(part + 1, p_start, p - p_start);
						part[p - p_start + 1] = '\0';
					}
					// strncpy_s(part, 128, p_start, p - p_start + 1);
					// strncpy(part, p_start, p - p_start + 1);
					DC_AddEndTTSInfo(part, segShow);
					APP_Printf("part: %s", part);
					p_start = p;
				}
				else
				{
					p = p_next + 1;
				}
			}
			else
			{
				// strcpy_s(part, 128, p_start);

				// strcpy(part, p_start);
				part[0] = '&';
				strcpy(part + 1, p_start);
				DC_AddEndTTSInfo(part, segShow);
				APP_Printf("part: %s", part);
			}
		}

	}
	else
		DC_AddEndTTSInfo(str, segShow);
}

