function prompt_search_result(result_prompt_id, libtype, msg)
{
	if (libtype == "LibAPI")
	{
		lib_string = "lib_api";
	}
	else if (libtype == "PaymentCore")
	{
		lib_string = "core_code";
	}
	result_prompt_id = result_prompt_id.toLowerCase();
	// document.getElementById(result_prompt_id + "_lib_api_search").readOnly = true;
	document.getElementById(result_prompt_id + "_" + lib_string + "_search_prompt").textContent = msg;
	setTimeout(function() {
		// Code to execute after the delay
		
		document.getElementById(result_prompt_id + "_" + lib_string + "_search").select();
		// document.getElementById(result_prompt_id).textContent = "";
