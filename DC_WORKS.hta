Function QueryRegistryValueAsString(sFullKeyPath)
	ValueString = ObjShell.RegRead(sFullKeyPath)
	If Err.Number <> 0 Then ' an error occurred
		QueryRegistryValueAsString = ""
	Else ' normal case
		QueryRegistryValueAsString = ValueString
	End If
End Function

Function WriteRegistryValue(sFullKeyPath, Value, sType)
	ObjShell.RegWrite sFullKeyPath, Value, sType
End Function


Function IsKeyExistent(sComputer, sKeyRoot, sKeyPath)
	Set ObjRegistry = GetObject("winmgmts:\\" & sComputer & "\root\default:StdRegProv")
	' If ObjRegistry.EnumKey(sKeyRoot, sKeyPath, aSubKeys) = 0 Then
	If ObjRegistry.EnumKey(sKeyRoot, sKeyPath) = 0 Then
		IsKeyExistent = True
	Else
		IsKeyExistent = False
	End If
End Function

Function CreateRegistryKey(sComputer, sNewKeyRoot, sNewKeyPath)
	' sComputer = "."
	Set ObjRegistry = GetObject("winmgmts:\\" & sComputer & "\root\default:StdRegProv")

	' Choose sNewKeyRoot from HKCU, HKLM, ..
	' Const HKCU = &H80000001
	' Const HKLM = &H80000002
	' Add "Persionalization" to This PC
	' sNewKeyPath = "SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\MyComputer\NameSpace\{ED834ED6-4B5A-4bfe-8F11-A626DCB6A921}"

	ObjRegistry.CreateKey sNewKeyRoot, sNewKeyPath
End Function

Function DeleteRegistryKey(sComputer, sNewKeyRoot, sNewKeyPath)
	Set ObjRegistry = GetObject("winmgmts:\\" & sComputer & "\root\default:StdRegProv")
	ObjRegistry.DeleteKey sNewKeyRoot, sNewKeyPath
End Function

Function AddItemToMyComputer(sCLSID, sComment)
	' CreateRegistryKey ".", &H80000002, "SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\MyComputer\NameSpace\" & sCLSID
	CreateRegistryKey ".", HKLM, "SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\MyComputer\NameSpace\" & sCLSID
End Function

Function RemoveItemFromMyComputer(sCLSID, sComment)
	DeleteRegistryKey ".", HKLM, "SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\MyComputer\NameSpace\" & sCLSID
End Function

