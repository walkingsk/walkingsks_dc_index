	Set ConfigFile = ObjFS.OpenTextFile(ScriptDIR & "\DC_WORKS.ini", 2, True)
	
	ConfigFile.WriteLine(HideS165xxMidVersions)
	
	ConfigFile.Close
End Sub

Sub KillGitExe()
	' From ChatGPT
	Dim objWMIService, colProcesses, objProcess

	' Create a WMI service object
	Set objWMIService = GetObject("winmgmts:\\.\root\cimv2")

	' Query for running processes
	Set colProcesses = objWMIService.ExecQuery("SELECT * FROM Win32_Process")

	' Iterate through each process
	For Each objProcess In colProcesses
		' Check if the command line contains "git fsmonitor--daemon run --detach --ipc-threads"
		If InStr(1, objProcess.CommandLine, "git fsmonitor--daemon run --detach --ipc-threads", vbTextCompare) > 0 Then
			' Found a matching process, terminate it
			objProcess.Terminate()
			' WScript.Echo "Process terminated: " & objProcess.CommandLine
		End If
	Next

	' Clean up
	Set objWMIService = Nothing
	Set colProcesses = Nothing
	Set objProcess = Nothing
End Sub

Sub Clean8910TracerLogs()
	Randomize
	' 0 1 2
	If Int(Rnd * 3) > 0 Then
		ObjShell.Run "CMD.exe /C CD /D " & ScriptDIR & "\Tools\8910_cooltools-win32_custom_P3.R2.0.0005-1\logs & IF EXIST *.TRC DEL *.TRC", 0, False
	End If
End Sub

Function GetSSURLToClipboard()
	ObjShell.Run "CMD.exe /C CD /D " & ScriptDIR & " & curl -s -L https://github.com/Alvin9999/new-pac/wiki/ss%E5%85%8D%E8%B4%B9%E8%B4%A6%E5%8F%B7 -x127.0.0.1:9910 -oSS_GitHub_URL_HTML.txt & cat SS_GitHub_URL_HTML.txt | grep ss:// | clip", 0, True
	
End Function
