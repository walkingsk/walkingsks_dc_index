Function GetCFileStringFromObjString(TargetResultObjFileName)
	GetCFileStringFromObjString = ""
	If Right(TargetResultObjFileName, 4) = ".obj" Then
		GetCFileStringFromObjString= Mid(TargetResultObjFileName, 1, Len(TargetResultObjFileName) - 4)
	ElseIf Right(TargetResultObjFileName, 2) = ".o" Then
		GetCFileStringFromObjString= Mid(TargetResultObjFileName, 1, Len(TargetResultObjFileName) - 2)
	End If
End Function
