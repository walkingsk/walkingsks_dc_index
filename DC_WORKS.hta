Sub ExplorerShellExecute(Path, Params, DIR, Verb, Window)
	' Path          The file to execute (required)
	' parameters    Arguments for the executable
	' dir           Working directory
	' verb          The operation to execute (runas/open/edit/print)
	' window        View mode application window (normal=1, hide=0, 2=Min, 3=max, 4=restore, 5=current, 7=min/inactive, 10=default)
	
	If DIR = -1 Then
		ObjApplication.ShellExecute Path, Params, ScriptDIR, Verb, Window
	Else
		ObjApplication.ShellExecute Path, Params, ScriptDIR & "\" & DIR, Verb, Window
	End If
End Sub
