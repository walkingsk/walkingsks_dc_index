		// console.log("Delayed code execution after 5 seconds");
	}, 5000);
}
function mock_progress_bar(project_name, libtype)
{
	if (libtype == "LibAPI")
	{
		lib_string = "lib_api";
	}
	else if (libtype == "PaymentCore")
	{
		lib_string = "core_code";
	}
	progress_bar = document.getElementById(project_name.toLowerCase() + "_" + lib_string + "_search_progress");
	
	progress_bar.removeAttribute("hidden");
	// progress_bar.style.width = "70px";
	
	setTimeout(function() {
		progress_bar.style.width = "7%";
		setTimeout(function() {
			progress_bar.style.width = "30%";
			setTimeout(function() {
				progress_bar.style.width = "70%";
				setTimeout(function() {
					progress_bar.style.width = "100%";
					progress_bar.setAttribute("hidden");
					progress_bar.style.width = "0%";
				}, 300);
			}, 600);
		}, 800);
	}, 100);
	
	
}
